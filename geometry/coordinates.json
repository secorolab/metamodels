{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "geom-coord-scr": "https://secorolab.github.io/metamodels/geometry/coordinates#",

        "VectorXY": "geom-coord-scr:VectorXY",
        "Quaternion": { "@id": "geom-coord-scr:Quaternion" },

        "EulerAngles": { "@id": "geom-coord-scr:EulerAngles" },
        "Intrinsic": { "@id": "geom-coord-scr:Intrinsic" },
        "Extrinsic": { "@id": "geom-coord-scr:Extrinsic" },
        "axes-sequence": {
            "@id": "geom-coord-scr:axes-sequence",
            "@type": "xsd:string"
        },

        "AnglesABG": { "@id": "geom-coord-scr:AnglesAlphaBetaGamma" },
        "alpha": {
            "@id": "geom-coord-scr:alpha",
            "@type": "xsd:double"
        },
        "beta": {
            "@id": "geom-coord-scr:beta",
            "@type": "xsd:double"
        },
        "gamma": {
            "@id": "geom-coord-scr:gamma",
            "@type": "xsd:double"
        }
    }
