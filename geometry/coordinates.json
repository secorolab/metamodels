{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "geom-coord": "https://comp-rob2b.github.io/metamodels/geometry/coordinates#",

        "VectorXY": "geom-coord:VectorXY",

        "Quaternion": { "@id": "geom-coord:Quaternion" },
        "w": { "@id": "geom-coord:w", "@type": "xsd:double" },

        "EulerAngles": { "@id": "geom-coord:EulerAngles" },
        "Intrinsic": { "@id": "geom-coord:Intrinsic" },
        "Extrinsic": { "@id": "geom-coord:Extrinsic" },
        "axes-sequence": {
            "@id": "geom-coord:axes-sequence",
            "@type": "xsd:string"
        },

        "AnglesABG": { "@id": "geom-coord:AnglesAlphaBetaGamma" },
        "alpha": {
            "@id": "geom-coord:alpha",
            "@type": "xsd:double"
        },
        "beta": {
            "@id": "geom-coord:beta",
            "@type": "xsd:double"
        },
        "gamma": {
            "@id": "geom-coord:gamma",
            "@type": "xsd:double"
        },
        "coordinates": {
            "@id": "geom-coord:coordinates",
            "@container": "@list"
        }
    }
}
