{
    "@context": {
        "poly": "https://hbrs-sesame.github.io/metamodels/polytope#",
        "Polytope": "poly:Polytope",
        "3DPolytope": "poly:3DPolytope",
        "Cuboid": "poly:Cuboid",
        "CuboidWithSize": "poly:CuboidWithSize",
        "x-size": {
            "@id": "poly:x-size",
            "@type": "xsd:double"
        },
        "y-size": {
            "@id": "poly:y-size",
            "@type": "xsd:double"
        },
        "z-size": {
            "@id": "poly:z-size",
            "@type": "xsd:double"
        },
        "points": {
            "@id": "poly:points",
            "@container": "@list",
            "@type": "@id"
        }
    }
}