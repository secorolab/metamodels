{
    "@context": {
        "poly": "https://secorolab.github.io/metamodels/geometry/polytope#",

        "Polytope": "poly:Polytope",
        "Polygon": "poly:Polygon",
        "Polyhedron": "poly:Polyhedron",
        "Circle": "poly:Circle",

        "points": {
            "@id": "poly:points",
            "@container": "@list",
            "@type": "@id"
        },
        "faces": {
            "@id": "poly:faces",
            "@container": "@list",
            "@type": "@id"
        },
        "3DPolytope": "poly:3DPolytope",
        "Cuboid": "poly:Cuboid",
        "CuboidWithSize": "poly:CuboidWithSize",
        "x-size": {
            "@id": "poly:x-size",
            "@type": "xsd:double"
        },
        "y-size": {
            "@id": "poly:y-size",
            "@type": "xsd:double"
        },
        "z-size": {
            "@id": "poly:z-size",
            "@type": "xsd:double"
        },
        "radius": {
            "@id": "poly:radius",
            "@type": "xsd:double"
        },
        "diameter": {
            "@id": "poly:diameter",
            "@type": "xsd:double"
        },
        "center": {
            "@id": "poly:center",
            "@type": "@id"
        },
        "Cylinder": {
            "@id": "poly:Cylinder",
            "@context": {
                "base": {
                    "@id": "poly:base",
                    "@type": "@id"
                },
                "height": {
                    "@id": "poly:height",
                    "@type": "xsd:double"
                },
                "axis": {
                    "@id": "poly:axis",
                    "@type": "xsd:double",
                    "@container": "@list"
                }
            }
        }
    }
}