{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "distrib": "https://secorolab.github.io/metamodels/probability/distribution#",

        "Continuous": { "@id": "distrib:Continuous" },
        "Discrete": { "@id": "distrib:Discrete" },

        "Distribution": { "@id": "distrib:Distribution" },
        "dimension": { "@id": "distrib:dimension", "@type": "xsd:positiveInteger" },

        "Uniform": { "@id": "distrib:Uniform" },
        "lower-bound": { "@id": "distrib:lower-bound", "@container": "@list", "@type": "xsd:decimal" },
        "upper-bound": { "@id": "distrib:upper-bound", "@container": "@list", "@type": "xsd:decimal" },

        "UniformRotation": { "@id": "distrib:UniformRotation" },

        "Normal": { "@id": "distrib:Normal" },
        "mean": { "@id": "distrib:mean", "@container": "@list", "@type": "xsd:decimal" },
        "std-dev": { "@id": "distrib:standard-deviation", "@type": "xsd:decimal" },
        "covariance": { "@id": "distrib:covariance", "@container": "@list", "@type": "xsd:decimal" },

        "SampledQuantity": { "@id": "distrib:SampledQuantity" },
        "from-distribution": { "@id": "distrib:from-distribution", "@type": "@id" }
    }
}
